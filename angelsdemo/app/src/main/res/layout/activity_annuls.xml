<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="@color/white_ac">

    <com.angels.widget.AnnulusView xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="200dp"
        android:layout_height="200dp"
        android:visibility="visible"
        app:annulusProgressColor="@android:color/holo_orange_dark"
        app:annulusRoundColor="@android:color/holo_blue_dark"
        app:annulusRoundWidth="20"
        app:annulusTextColor="@android:color/black"
        app:annulusTextIsShow="true"
        app:annulusTextSize="14sp" />
    <com.angels.widget.AnnulusView2 xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="200dp"
    android:layout_height="200dp"
    android:visibility="visible"
    app:annulusProgressColor="@android:color/holo_orange_dark"
    app:annulusRoundColor="@android:color/holo_blue_dark"
    app:annulusRoundWidth="30"
    app:annulusTextColor="@android:color/black"
    app:annulusTextIsShow="true"
    app:annulusTextSize="16sp"
    android:background="@color/green_ac"/>

    <com.angels.widget.AnnulusView2
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:visibility="visible"/>
</LinearLayout>