<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="42dp"
    android:gravity="center_vertical"
    android:background="@color/black_ac"
    android:id="@+id/ac_rl_web_title">
    <TextView
        android:id="@+id/ac_tv_back"
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        android:drawableLeft="@drawable/ac_back_white"
        android:text="返回"
        android:textColor="@color/white_ac"
        android:drawablePadding="5dp"
        android:gravity="center"
        android:textSize="16dp"
        android:background="@drawable/ac_effect_bg"
        android:padding="10dp"/>
    <TextView
        android:id="@+id/ac_tv_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="标题"
        android:textColor="#ffffff"
        android:gravity="center"
        android:layout_centerInParent="true"
        android:textSize="18dp"
        android:singleLine="true"
        android:ems="10"
        android:ellipsize="end"/>
</RelativeLayout>